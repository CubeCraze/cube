{
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 140,
  "tabWidth": 2,
  "semi": false,
  "quoteProps": "consistent",
  "arrowParens": "always",
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [
    { "files": "*.svelte", "options": { "parser": "svelte" } },
    { "files": "*.svg", "options": { "parser": "html" } }
  ]
}
